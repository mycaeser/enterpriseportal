<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.caeser.enterpriseportal.dao.RegistDao">
	<select id="queryRegistList" resultType="com.caeser.enterpriseportal.entity.Regist">
		SELECT  aac501,aac502,aac503,aac504,aac505,aac506
		FROM ac05
	</select>
	<select id="queryRegistById" resultType="com.caeser.enterpriseportal.entity.Regist">
		SELECT  aac501,aac502,aac503,aac504,aac505,aac506
		FROM ac05
		WHERE
		aac501=#{id}
	</select>
	<insert id="addRegist" useGeneratedKeys="true"
		keyProperty="Aac501" keyColumn="aac501">
		INSERT INTO
		ac05(aac502,aac503,aac504,aac505,aac506)
		VALUES
		(#{Aac502},#{Aac503},#{Aac504},#{Aac505},#{Aac506})
	</insert>
	<update id="updateRegist" parameterType="com.caeser.enterpriseportal.entity.Regist">
		update ac05
		<set>
			<if test="Aac502 != null">aac502=#{Aac502},</if>
			<if test="Aac503 != null">aac503=#{Aac503},</if>
			<if test="Aac504 != null">aac504=#{Aac504},</if>
			<if test="Aac505 != null">aac505=#{Aac505},</if>
			<if test="Aac506 != null">aac506=#{Aac506}</if>
		</set>
		where aac501=#{Aac501}
	</update>
	<delete id="deleteRegistById" parameterType="int">
		DELETE FROM
		ac05
		WHERE aac501=#{id}
	</delete>
</mapper>
    