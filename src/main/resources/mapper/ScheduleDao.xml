<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.caeser.enterpriseportal.dao.ScheduleDao">
	<select id="queryScheduleList" resultType="com.caeser.enterpriseportal.entity.Schedule">
		SELECT  aac701,aac702
		FROM ac07
	</select>
	<select id="queryScheduleById" resultType="com.caeser.enterpriseportal.entity.Schedule">
		SELECT  aac701,aac702
		FROM ac07
		WHERE
		aac701=#{id}
	</select>
	<insert id="addSchedule" useGeneratedKeys="true"
		keyProperty="Aac701" keyColumn="aac701">
		INSERT INTO
		ac07(aac702)
		VALUES
		(#{aac702})
	</insert>
	<delete id="deleteScheduleById" parameterType="int">
		DELETE FROM
		ac07
		WHERE aac701=#{id}
	</delete>
	<update id="updateScheduleById" parameterType="com.caeser.enterpriseportal.entity.Schedule">
		update ac07
		SET
		aac702=#{Aac702}
		where aac701=#{Aac701}
	</update>
</mapper>
    